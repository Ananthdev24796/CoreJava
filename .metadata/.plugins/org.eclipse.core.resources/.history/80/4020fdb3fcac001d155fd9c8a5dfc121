package com.bridgelabz;

public class TestFinal1 {
	
	
	// instance variable 
	int a = 100;
	// static variable
	static int b = 200 ;
	
	// static method
	public static void main(String[] args ) {  // main method
		TestFinal1 t1 = new TestFinal1(); 
		System.out.println(t1.a);
		System.out.println(TestFinal1.b);
		
	}
	

}
