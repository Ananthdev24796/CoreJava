package com.bridgelabz;

public class LinearSearch {
	public static void main(String[] args) {
		int[] array = { 10,20,30,40,50};
		int key = 40;
		
		
		for ( int i = 0 ;i < array.length-1;i++) {
			if(array[i] == key) {
				System.out.println(key + i);
			}
			else {
				System.out.println(key + " not found");
			}
		}
	}
}
	

